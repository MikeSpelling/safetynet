<?xml version="1.0"?>
<slideshow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation='g1_schema_v1.4.4.xsd'>
  <documentinfo>
    <author>Michael Spelling</author>
    <version>1.0</version>
    <comment>A Text testing xml file. Complies with schema.xsd</comment>
  </documentinfo>
  <defaults>
    <textdefault  fontname="Arial" fontsize="12" fontcolor="#000000" />
    <slidedefault backgroundcolor="#000000" />
  </defaults>

  <slide id="1" backgroundcolor="#ffffff" duration="50000" nextslideid="1">
    <text fontname="Arial" fontsize="0" fontcolor="#ffffff" alpha="1.0" x="0" y="0" starttime="0" endtime="0">TextBox1</text>
    <text fontname="serif" fontsize="18" fontcolor="#aaaaaa" alpha="0.1" x="0" y="100" starttime="0" endtime="0">TextBox2</text>
    <text fontname="sans serif" fontsize="12" fontcolor="#000fff" alpha="1.0" x="0" y="200" starttime="0" endtime="0">TextBox3</text>
    <text fontname="times new roman" fontsize="20" fontcolor="#fff000" alpha="1.0" x="0" y="300" starttime="0" endtime="0">TextBox4</text>
    <text fontname="Arial bold" fontsize="30" fontcolor="#123456" alpha="0.5" x="0" y="400" starttime="0" endtime="0">TextBox5</text>
    <text fontname="Arial italic" fontsize="40" fontcolor="#654321" alpha="1.0" x="300" y="0" starttime="0" endtime="0">TextBox6</text>
    <text fontname="windings" fontsize="25" fontcolor="#100000" alpha="0.8" x="300" y="100" starttime="0" endtime="0">TextBox7</text>
    <text fontname="Arial" fontsize="14" fontcolor="#001000" alpha="1.0" x="300" y="200" starttime="0" endtime="0">TextBox8</text>
    <text fontname="Arial" fontsize="16" fontcolor="#ffffff" alpha="1.0" x="300" y="300" starttime="0" endtime="0">TextBox9</text>
    <text fontname="Arial" fontsize="100" fontcolor="#ffffff" alpha="1.0" x="300" y="400" starttime="0" endtime="0">TextBox10</text>
  </slide>
  </slideshow>